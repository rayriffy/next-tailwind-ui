{
  "name": "next-tailwind-ui",
  "version": "1.0.0",
  "description": "Complete Next template for repid development",
  "repository": "git@github.com:rayriffy/next-tailwind-ui.git",
  "author": "Phumrapee Limpianchop <contact@rayriffy.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "yarn --silent dev:debugger",
    "dev:normal": "next",
    "dev:debugger": "NODE_OPTIONS='--inspect' next dev",
    "start": "next start"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.4.0",
    "@heroicons/react": "^1.0.3",
    "@next/bundle-analyzer": "^11.0.1",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/typography": "^0.4.1",
    "@zeit/next-workers": "^1.0.0",
    "dayjs": "^1.10.6",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.1.3",
    "next": "^11.0.1",
    "next-compose-plugins": "^2.2.1",
    "next-offline": "^5.0.5",
    "next-plugin-preact": "^3.0.6",
    "postcss": "^8.3.6",
    "preact": "^10.5.14",
    "preact-render-to-string": "^5.1.19",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "react-ssr-prepass": "npm:preact-ssr-prepass",
    "storeon": "^3.1.4",
    "swr": "^1.2.0",
    "tailwindcss": "^2.2.7",
    "worker-loader": "^3.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/lodash": "^4.14.171",
    "@types/markdown-to-jsx": "^7.0.1",
    "@types/node": "^16.4.10",
    "@types/react": "^17.0.15",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.0.1",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  }
}
