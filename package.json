{
  "name": "next-tailwind-ui",
  "version": "1.0.0",
  "description": "Complete Next template for repid development",
  "repository": "git@github.com:rayriffy/next-tailwind-ui.git",
  "author": "Phumrapee Limpianchop <contact@rayriffy.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "yarn --silent dev:debugger",
    "dev:normal": "next",
    "dev:debugger": "NODE_OPTIONS='--inspect' next dev",
    "start": "next start"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^0.2.0",
    "@next/bundle-analyzer": "^10.0.4",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.3.1",
    "@zeit/next-workers": "^1.0.0",
    "dayjs": "^1.9.7",
    "lodash": "^4.17.20",
    "markdown-to-jsx": "^7.1.0",
    "next": "^10.0.3",
    "next-compose-plugins": "^2.2.1",
    "next-offline": "^5.0.3",
    "next-plugin-preact": "^3.0.3",
    "postcss": "^8.2.1",
    "preact": "^10.5.7",
    "preact-render-to-string": "^5.1.12",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "storeon": "^3.1.2",
    "swr": "^0.3.11",
    "tailwindcss": "^2.0.2",
    "worker-loader": "^3.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/lodash": "^4.14.165",
    "@types/markdown-to-jsx": "^6.11.3",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}
